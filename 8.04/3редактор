x=0
y=300
myText=0
def setup():
    global x,y,myText
    fill(155,33,33)
    background(255)
    textSize(30)
    size(600,600)
    myText=u"я люблю програмирование"
def draw():
    global x,y
    
def keyPressed():
    global x,y,myText

    
    #text(myText,150,200)
    #x=x+15
    if(key==ENTER):
        y=y+30
        x=0
    elif(key==TAB):
        x=x+40
    else:
        
        myText+=key
        text(myText,x,y)
        x=x+15
        
    
