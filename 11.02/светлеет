x=300
y=250
w=80
d=80
angle=0
def setup():
    global x,y,w,d,angle
    size(600,500)
    colorMode(HSB,360,100,100)
    fill(0,0,angle)
    frameRate(10)
    

def draw():
    global x,y,w,d,angle
    fill(0,0,angle)
    ellipse(x,y,w,d)
    angle = angle + 5
    if (angle == 100):
        angle = 0
    
