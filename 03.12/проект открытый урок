ellipse(120,110,60,60)
fill(227,192,53)

x=0
y=0
w=0
d=0
xChange=0
yChange=0
def setup():
    global x,y,w,d,xChange,yChange
    size(750,600)
    fill(37,227,21)
    frameRate(1)
    background(255)
    x = 120
    y = 180
    w = 100
    d = 90
    xChange = 10
    yChange = 10
def draw():
    global x,y,w,d,xChange,yChange
    ellipse(x,y,w,d)
    background(255)
    x = x + xChange
    y = y + yChange
    
