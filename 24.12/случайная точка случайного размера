x=0
y=0
xChange=0
yChange=0

def setup():
    global x,y,x,xChange,yChange
    size(600,400)
    background(255)
    frameRate(5)
    strokeWeight(random(0,200))
    x = (random(0,100))
    y = (random(0,120))
    xChange=1
    yChange=1
def draw():
    global x,y,r,rChange
    background(255)
    point(x,y)
    strokeWeight(random(0,200))
    x = random(0,500)
    y = random(0,600)
    
